name "dynaload"
description "add classes and methods from a dynamic library"
authors "Jean-Louis Leroy"
copyright "Copyright Â© 2017, Jean-Louis Leroy"
license "Boost Software License 1.0"
targetType "executable"
dependency "openmethods" path="../../"
dflags "-defaultlib=phobos2"

buildType "x" {
  buildOptions "debugMode" "debugInfo" "unittests"
  debugVersions "explain"
}

buildType "xtc" {
  buildOptions "debugMode" "debugInfo" "unittests"
  debugVersions "explain" "traceCalls"
}
